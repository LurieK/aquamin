{
	"name": "aquamin",
	"version": "3.0.2",
	"description": "Parcel build tool for Aquamarine",
	"private": true,
	"engines": {
		"nodeVersion": ">=16.13.2",
		"npmVersion": ">=7.11.2"
	},
	"config": {
		"lint_paths": "./blocks ./components",
		"asset_paths": "./*/*.bundle.js ./blocks/block-library/*/*script.js ./blocks/block-library/*/*style.css"
	},
	"scripts": {
		"start": "npm-run-all --parallel builder server",
		"build": "npm run clean && parcel build $npm_package_config_asset_paths --no-cache && npm run build:post",
		"build:post": "npm run build:assets; npm run lint",
		"build:assets": "del ./dist/assets/* && ncp ./components/assets ./dist/assets --filter '**/*.*'",
		"server": "browser-sync start --logFileChanges=`grep LOG_FILE_CHANGES= .env | cut -d '=' -f2` --no-open --proxy `grep URL= .env | cut -d '=' -f2` --files './dist/**/*.css,./dist/**/*.js,./**/*.php'",
		"docs:server": "browser-sync start --no-open --proxy='http://127.0.0.1:4001/aquamin/' --files='./docs/'",
		"docs": "cd ./docs; jekyll serve --config _config_local.yml --port 4001",
		"builder": "parcel watch $npm_package_config_asset_paths",
		"lint": "eslint $npm_package_config_lint_paths",
		"lint:fix": "eslint $npm_package_config_lint_paths --fix",
		"clean": "del ./.parcel-cache/*"
	},
	"author": "@tcmulder",
	"license": "ISC",
	"@parcel/transformer-css": {
		"drafts": {
			"nesting": true,
			"customMedia": true
		}
	},
	"browserslist": "> 0.25%, last 2 versions, not dead",
	"devDependencies": {
		"@babel/core": "^7.22.8",
		"@babel/eslint-parser": "^7.22.7",
		"@babel/preset-react": "^7.22.5",
		"@parcel/resolver-glob": "^2.9.3",
		"@parcel/transformer-inline-string": "^2.9.3",
		"@parcel/transformer-sass": "^2.9.3",
		"@parcel/transformer-svg-react": "^2.9.3",
		"@types/node": "^20.4.1",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@wordpress/icons": "^9.28.0",
		"browser-sync": "^2.29.3",
		"classnames": "^2.3.2",
		"del-cli": "^5.0.0",
		"eslint": "^8.26.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-wesbos": "^3.1.4",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ncp": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.9.3",
		"prettier": "^2.7.1",
		"process": "^0.11.10",
		"react": "^18.2.0",
		"typescript": "^5.1.6"
	}
}
