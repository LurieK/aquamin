{
	"name": "aquamin",
	"version": "3.0.1",
	"description": "Parcel build tool for Aquamarine",
	"private": true,
	"engines": {
		"nodeVersion": ">=16.13.2",
		"npmVersion": ">=7.11.2"
	},
	"scripts": {
		"start": "npm-run-all --parallel builder server",
		"build": "npm run clean && parcel build ./*/*.bundle.js --no-cache && npm run build:post",
		"build:post": "ncp ./components/assets ./dist/assets --filter '**/*.*'",
		"server": "browser-sync start --logFileChanges=`grep LOG_FILE_CHANGES= .env | cut -d '=' -f2` --no-open --proxy `grep URL= .env | cut -d '=' -f2` --files './dist/**/*.css,./dist/**/*.js,./**/*.php'",
		"docs:server": "browser-sync start --no-open --proxy='http://127.0.0.1:4001/aquamin/' --files='./docs/'",
		"docs": "cd ./docs; jekyll serve --config _config_local.yml --port 4001",
		"builder": "parcel watch ./*/*.bundle.js",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"clean": "rm -rf ./.parcel-cache"
	},
	"author": "@tcmulder",
	"license": "ISC",
	"@parcel/transformer-css": {
		"drafts": {
			"nesting": true,
			"customMedia": true
		}
	},
	"devDependencies": {
		"@babel/core": "^7.19.6",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/preset-react": "^7.18.6",
		"@parcel/resolver-glob": "^2.7.0",
		"@parcel/transformer-inline-string": "^2.7.0",
		"@parcel/transformer-sass": "^2.7.0",
		"@parcel/transformer-svg-react": "^2.7.0",
		"@types/node": "^18.11.7",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"@wordpress/icons": "^9.11.0",
		"browser-sync": "^2.27.10",
		"classnames": "^2.3.2",
		"eslint": "^8.26.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-wesbos": "^3.1.4",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ncp": "2.0.0",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.7.0",
		"prettier": "^2.7.1",
		"process": "^0.11.10",
		"react": "^18.2.0",
		"typescript": "^4.8.4"
	}
}
