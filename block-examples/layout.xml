  <content:encoded><![CDATA[<!-- wp:html -->
<div class="size-check">
	<style>
		.torso h3 {
			margin-top: 25px !important;
			margin-bottom: 25px !important;
		}
		.torso .wp-block-group,
		.torso .wp-block-group__inner-container,
		.torso .wp-block-columns,
		.torso .wp-block-column {
			outline: 1px solid rgba(0, 0, 0, 0.2);
		}
		.torso .has-background {
			background-color: whitesmoke !important;
		}
		.torso .has-background .has-background {
			background-color: lightgray !important;
		}
		.size-check {
			margin-top: 250px;
			width: 100% !important;
			max-width: 100% !important;
		}
		
		.size-check div {
			background: currentcolor;
			margin: auto;
			position: relative;
		}
		
		.size-check span {
			float: right;
		}
		
		.size-check i {
			border: 1px dashed currentcolor;
			top: 0;
			height: 9999px;
			position: absolute;
			width: var(--gutter);
		}
		
		.size-check i:first-of-type {
			right: 100%;
		}
		
		.size-check i:last-of-type {
			left: 100%;
		}
		
		.size-check i {
			opacity: 0.5;
		}
		
		.size-check b {
			color: black;
		}
		
		.size-check__container {
			padding: 0 var(--gutter);
		}
		
		.size-check__container--normal,
		.size-check__container--normal div {
			color: aquamarine;
			width: 100% !important;
			max-width: var(--w-default) !important;
		}
		
		.size-check__container--wide,
		.size-check__container--wide div {
			color: mediumaquamarine;
			width: 100% !important;
			max-width: var(--w-wide) !important;
		}
	</style>
	<div class="size-check__container size-check__container--normal">
		<div>
			<i></i><i></i>
			<b>[ Normal Column Reference<span>]</span></b>
		</div>
		<i></i><i></i>
	</div>
	<div class="size-check__container size-check__container--wide">
		<div>
			<i></i><i></i>
			<b>[ Wide Column Reference<span>]</span></b>
		</div>
		<i></i><i></i>
	</div>
</div>
<!-- /wp:html -->

<!-- wp:heading {"level":3} -->
<h3><em>by default, groups are just column width</em>, <em>soâ€¦</em></h3>
<!-- /wp:heading -->

<!-- wp:group {"backgroundColor":"black-dark"} -->
<div class="wp-block-group has-black-dark-background-color has-background"><!-- wp:heading {"level":3,"backgroundColor":"yellow"} -->
<h3 class="has-yellow-background-color has-background">...normal inner blocks are this width,</h3>
<!-- /wp:heading -->

<!-- wp:heading {"level":3,"backgroundColor":"yellow"} -->
<h3 class="has-yellow-background-color has-background">wide inner blocks are this width,</h3>
<!-- /wp:heading -->

<!-- wp:heading {"level":3,"backgroundColor":"yellow"} -->
<h3 class="has-yellow-background-color has-background">and full inner blocks are this width.</h3>
<!-- /wp:heading --></div>
<!-- /wp:group -->

<!-- wp:heading {"level":3} -->
<h3><em>But, if you set a root-level group to full width...</em></h3>
<!-- /wp:heading -->

<!-- wp:group {"align":"full","backgroundColor":"black-dark","layout":{"inherit":true}} -->
<div class="wp-block-group alignfull has-black-dark-background-color has-background"><!-- wp:heading {"level":3,"backgroundColor":"yellow"} -->
<h3 class="has-yellow-background-color has-background">...normal inner blocks are this width,</h3>
<!-- /wp:heading -->

<!-- wp:heading {"level":3,"align":"wide","backgroundColor":"yellow"} -->
<h3 class="alignwide has-yellow-background-color has-background">wide inner blocks are this width,</h3>
<!-- /wp:heading -->

<!-- wp:heading {"level":3,"align":"full","backgroundColor":"yellow"} -->
<h3 class="alignfull has-yellow-background-color has-background">and full inner blocks are this width.</h3>
<!-- /wp:heading --></div>
<!-- /wp:group -->

<!-- wp:heading {"level":3} -->
<h3><em>And, you can remove the default side gutters, which means...</em></h3>
<!-- /wp:heading -->

<!-- wp:group {"align":"full","backgroundColor":"black-dark","layout":{"inherit":true},"aquaminClassNameContainer":[{"value":"is-style-no-gutters","title":"Remove side gutters"}]} -->
<div class="wp-block-group alignfull has-black-dark-background-color has-background is-style-no-gutters"><!-- wp:heading {"level":3,"backgroundColor":"yellow"} -->
<h3 class="has-yellow-background-color has-background">...normal inner blocks are this width,</h3>
<!-- /wp:heading -->

<!-- wp:heading {"level":3,"align":"wide","backgroundColor":"yellow"} -->
<h3 class="alignwide has-yellow-background-color has-background">wide inner blocks are this width,</h3>
<!-- /wp:heading -->

<!-- wp:heading {"level":3,"align":"full","backgroundColor":"yellow"} -->
<h3 class="alignfull has-yellow-background-color has-background">and full inner blocks are this width.</h3>
<!-- /wp:heading --></div>
<!-- /wp:group -->

<!-- wp:heading {"level":3} -->
<h3><em>Or if you remove side gutters in a normal root-level group, then...</em></h3>
<!-- /wp:heading -->

<!-- wp:group {"backgroundColor":"black-dark","layout":{"inherit":true},"aquaminClassNameContainer":[{"value":"is-style-no-gutters","title":"Remove side gutters"}]} -->
<div class="wp-block-group has-black-dark-background-color has-background is-style-no-gutters"><!-- wp:heading {"level":3,"backgroundColor":"yellow"} -->
<h3 class="has-yellow-background-color has-background">...normal inner blocks are this width,</h3>
<!-- /wp:heading -->

<!-- wp:heading {"level":3,"align":"wide","backgroundColor":"yellow"} -->
<h3 class="alignwide has-yellow-background-color has-background">wide inner blocks are this width,</h3>
<!-- /wp:heading -->

<!-- wp:heading {"level":3,"align":"full","backgroundColor":"yellow"} -->
<h3 class="alignfull has-yellow-background-color has-background">and full inner blocks are this width.</h3>
<!-- /wp:heading --></div>
<!-- /wp:group -->

<!-- wp:heading {"level":3} -->
<h3><em>You can also customize inner block widths, which could mean...</em></h3>
<!-- /wp:heading -->

<!-- wp:group {"align":"full","backgroundColor":"black-dark","layout":{"inherit":false,"contentSize":"500px","wideSize":"1400px"}} -->
<div class="wp-block-group alignfull has-black-dark-background-color has-background"><!-- wp:heading {"level":3,"backgroundColor":"yellow"} -->
<h3 class="has-yellow-background-color has-background">...normal inner blocks are this width,</h3>
<!-- /wp:heading -->

<!-- wp:heading {"level":3,"align":"wide","backgroundColor":"yellow"} -->
<h3 class="alignwide has-yellow-background-color has-background">wide inner blocks are this width,</h3>
<!-- /wp:heading -->

<!-- wp:heading {"level":3,"align":"full","backgroundColor":"yellow"} -->
<h3 class="alignfull has-yellow-background-color has-background">and full inner blocks are this width.</h3>
<!-- /wp:heading --></div>
<!-- /wp:group -->

<!-- wp:heading {"level":3} -->
<h3><em>Lastly, you can set nested layouts within inner blocks, which could mean...</em></h3>
<!-- /wp:heading -->

<!-- wp:group {"align":"full","backgroundColor":"black-dark","layout":{"inherit":true}} -->
<div class="wp-block-group alignfull has-black-dark-background-color has-background"><!-- wp:columns -->
<div class="wp-block-columns"><!-- wp:column {"width":"100%","layout":{"contentSize":"600px","wideSize":"600px"}} -->
<div class="wp-block-column" style="flex-basis:100%"><!-- wp:heading {"level":3,"backgroundColor":"yellow"} -->
<h3 class="has-yellow-background-color has-background">...normal inner blocks are this width,</h3>
<!-- /wp:heading --></div>
<!-- /wp:column --></div>
<!-- /wp:columns -->

<!-- wp:group {"align":"wide","layout":{"inherit":false,"contentSize":"600px","wideSize":"600px"}} -->
<div class="wp-block-group alignwide"><!-- wp:heading {"level":3,"align":"wide","backgroundColor":"yellow"} -->
<h3 class="alignwide has-yellow-background-color has-background">wide inner blocks are this width,</h3>
<!-- /wp:heading --></div>
<!-- /wp:group -->

<!-- wp:columns {"align":"full"} -->
<div class="wp-block-columns alignfull"><!-- wp:column {"width":"100%","layout":{"contentSize":"600px"}} -->
<div class="wp-block-column" style="flex-basis:100%"><!-- wp:heading {"level":3,"backgroundColor":"yellow"} -->
<h3 class="has-yellow-background-color has-background">and full inner blocks are this width.</h3>
<!-- /wp:heading --></div>
<!-- /wp:column --></div>
<!-- /wp:columns --></div>
<!-- /wp:group -->

<!-- wp:paragraph -->
<p></p>
<!-- /wp:paragraph -->]]></content:encoded>
